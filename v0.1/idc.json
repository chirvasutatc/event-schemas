{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Identifier List Schema",
  "type": "object",
  "required": [
    "newIdentifiers"
  ],
  "properties": {
    "newIdentifiers": {
      "type": "array",
      "description": "A list of identifiers in 'type:identifier' format.",
      "items": {
        "type": "string",
        "pattern": "^[^:]+:[^:]+$"
      }
    },
    "metadata": {
      "type": "string",
      "description": "A string of metadata about the event."
    }
  }
}